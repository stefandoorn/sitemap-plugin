{
    "name": "stefandoorn/sylius-sitemap-bundle",
    "type": "sylius-bundle",
    "description": "Sitemap bundle for Sylius",
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0",
        "sylius/sylius": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^0.3",
        "friends-of-behat/cross-container-extension": "^0.2",
        "friends-of-behat/performance-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^0.2.1",
        "friends-of-behat/variadic-extension": "^0.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "se/selenium-server-standalone": "^2.52",
        "phpunit/phpunit": "^5.0",
        "lakion/api-test-case": "^1.1",
        "matthiasnoback/symfony-dependency-injection-test": "^2.0",
        "raveren/kint": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SyliusSitemapBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SyliusSitemapBundle\\": "tests/"
        }
    }
}
