{
    "name": "stefandoorn/sitemap-plugin",
    "type": "sylius-plugin",
    "description": "Sitemap Plugin for Sylius",
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0",
        "sylius/sylius": "^1.0@beta"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^5.0",
        "lakion/api-test-case": "^1.1",
        "raveren/kint": "^1.1",
        "matthiasnoback/symfony-dependency-injection-test": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SitemapPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SitemapPlugin\\": "tests/"
        }
    }
}
